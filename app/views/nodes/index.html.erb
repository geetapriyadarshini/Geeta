<body bgcolor="sky blue">
<h2>Go To</h2>
<%#= link_to 'me', geeta_me_path %>
<%= link_to 'new form', new_node_path %>

<h1>Listing nodes</h1>
<table>
  <tr>
    <th>node_Name</th>
    <th>Place_Id</th>
    <th colspan="3">Show and Edit</th>
  </tr>
 
  <% @nodes.each do |node| %>
    <tr>
      <td><%= node.node_name %></td>
      <td><%= node.place_id %></td>
     
      <td><%= link_to 'Show', node_path(node) %></td>
      <td><%= link_to 'Edit', edit_node_path(node) %></td>
      <td><%= link_to 'Delete', node_path(node),
              method: :delete,
              data: { confirm: 'Are you sure?' } %></td>
    </tr>
  <% end %>
</table>
<%#= link_to 'Show', company_path(:company) %>
</body>
