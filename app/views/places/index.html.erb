<body bgcolor="sky blue">
<h2>Go To</h2>
<%#= link_to 'me', geeta_me_path %>
<%= link_to 'new form', new_place_path %>

<h1>Listing places</h1>
<table>
  <tr>
    <th>Place Name</th>
    <th>company_id</th>
    <th colspan="3">Show and Edit</th>
  </tr>
 
  <% @places.each do |place| %>
    <tr>
      <td><%= place.place_name %></td>
      <td><%= place.company_id %></td>
      <td><%= link_to 'Show', state_path(place) %></td>
      <td><%= link_to 'Edit', edit_place_path(place) %></td>
      <td><%= link_to 'Delete', place_path(place),
              method: :delete,
              data: { confirm: 'Are you sure?' } %></td>
    </tr>
  <% end %>
</table>
</body>
